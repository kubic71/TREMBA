{
  "batch_size": 1,
  "train_path": "./dataset/Cat",
  "data_path": "./dataset/Cat",
  "epsilon": 0.05,
  "margin": 5.0,
  "target": false,
  "generator_name": "Imagenet_VGG16_Resnet18_Squeezenet_Googlenet_untarget",

